{
  "name": "@extension/honeycomb",
  "version": "0.3.3",
  "description": "chrome extension - new tab",
  "private": true,
  "sideEffects": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean:node_modules": "pnpx rimraf node_modules",
    "clean:turbo": "rimraf .turbo",
    "clean": "pnpm clean:turbo && pnpm clean:node_modules",
    "build": "vite build",
    "dev": "cross-env __DEV__=true vite dev --mode development",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@extension/i18n": "workspace:*",
    "@extension/shared": "workspace:*",
    "@extension/storage": "workspace:*",
    "@extension/ui": "workspace:*",
    "@react-aria/utils": "^3.25.3",
    "@tanstack/react-router": "^1.79.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@xstate/store": "^2.6.0",
    "cmdk": "^1.0.4",
    "effect": "^3.10.12",
    "framer-motion": "^11.11.11",
    "justd-icons": "^1.7.2",
    "react-aria": "^3.35.1",
    "react-aria-components": "^1.4.1"
  },
  "devDependencies": {
    "@extension/tailwindcss-config": "workspace:*",
    "@extension/tsconfig": "workspace:*",
    "@extension/vite-config": "workspace:*",
    "@tanstack/router-devtools": "^1.79.0",
    "@tanstack/router-plugin": "^1.79.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "postcss-load-config": "^6.0.1",
    "sass": "1.79.4",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^1.1.6"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
